{
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "rules": {
      "prefer-arrow-callback": "warn",
      "no-unused-vars": "warn",
      "no-duplicate-imports": "error",
      "no-var": "error",
      "no-use-before-define": "error",
      "no-unreachable": "error",
      "valid-typeof": "error",
      "no-undef": "error",
      "no-empty": "error",
      "no-const-assign": "error"
    }
  }